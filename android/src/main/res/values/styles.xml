<resources>

    <style name="AppTheme.CircularProgress" parent="Material.Drawable.CircularProgress">
        <item name="cpd_strokeColor">@color/progress_bar_stroke</item>
    </style>
    <style name="AppTheme.History.FloatingButton">
        <item name="fab_backgroundColor">?colorAccent</item>
        <item name="rd_enable">true</item>
        <item name="rd_style">@style/LightFABWave</item>
        <item name="fab_elevation">6dp</item>
    </style>
    <style name="AppTheme.Toolbar.Title" parent="Base.TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textColor">@color/default_light</item>
    </style>
    <style name="AppTheme.Toolbar" parent="Base.Widget.AppCompat.Toolbar">
        <item name="android:background">?attr/colorPrimary</item>
    </style>
</resources>
